# -*- mode: python -*-

import utils
from build import build_lib

Import('env')
ostype = utils.ostype()

# Build rule to create version file
build_tag = env.Command(
    'global/.caelus_build',
    [],
    utils.tag_build_version)
env.AlwaysBuild(build_tag)
version_file = env.Command(
    'global/global.cpp',
    [ 'global/global.Cver', build_tag[0] ],
    utils.stamp_caelus_version)

# Build STL ASCII reader
_stl_path = 'meshTools/triSurface/triSurface/interfaces/STL/'
if ostype == "windows":
    src_from_flex = [_stl_path+'windows/readSTLASCII.cpp']
else:
    src = _stl_path+'readSTLASCII.ll'
    tgt = _stl_path+'posix/readSTLASCII.cpp'
    flex_env = env.Clone()
    flex_env.CXXFile(tgt, src, LEXFLAGS='-+')
    src_from_flex = [tgt]

