    inlet
    {
        type            zeroGradient;
    }

    outlet
    {
        type            fixedValue;
        value           $internalField;
    }

    lowerWall
    {
        type            zeroGradient;
    }

    "motorBike_.*"
    {
        type            zeroGradient;
    }

    #include "include/frontBackUpperPatches"
